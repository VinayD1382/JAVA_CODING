
public class Main
{
	public static void main(String[] args) {
		String str1 = "VINAY";
		String str2 = "NAVY";
		boolean res = palindrom(str1,str2);
	}
	
	public boolean palindrom(String str1,String str2){
	    str1 = str1.toLowerCase();
	    str2  = str2.toLowerCase();
	    for(str1.length()!=str2.length()){
	        return false;
	    }
	    for(int i=0;i<str1.length()-1;i++){
	        System.out.priintln("Welcome");
	    }
	}
}
