Patten 17 : 
**********
****  ****
***    ***
**      **
*        *
class Main{
    public static void main(String[] args){
        int n = 5;
        for(int i=1;i<=n;i++){
            for(int j=n;j>=i;j--){
                System.out.print("*");
            }
            int s = 2*(i-1);
            for(int h=1;h<=s;h++){
                System.out.print(" ");
            }
            for(int k=n;k>=i;k--){
                System.out.print("*");
            }
            System.out.println();
        }

Pattern 18 : 
**********
****  ****
***    ***
**      **
*        *
*        *
**      **
***    ***
****  ****
**********
class Main{
    public static void main(String[] args){
        int n = 5;
        for(int i=1;i<=n;i++){
            for(int j=n;j>=i;j--){
                System.out.print("*");
            }
            int s = 2*(i-1);
            for(int h=1;h<=s;h++){
                System.out.print(" ");
            }
            for(int k=n;k>=i;k--){
                System.out.print("*");
            }
            System.out.println();
        }
           for(int i=n;i>=1;i--){
            for(int j=n;j>=i;j--){
                System.out.print("*");
            }
            int s = 2*(i-1);
            for(int h=1;h<=s;h++){
                System.out.print(" ");
            }
            for(int k=n;k>=i;k--){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}

