class Main {
    public static void main(String[] args) {
        int count = 1;
      for(int i=1;i<=5;i++){
          for(int j=1;j<=5;j++){
              if(j%2==0){
              System.out.print(i);
          }else{
              System.out.print(j);
          }
         
      }
       System.out.println();
    }
}
}
Pattern 7 : 
_-_-_
-_-_-
_-_-_
-_-_-
_-_-_
class Main{
    public static void main(String[] args){
        int n = 5;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n;j++){
                if(i%2==0){
                    System.out.print(j%2==0 ? "_" : "-");
                }else{
                    System.out.print(j%2==0 ? "-" : "_");
                }
            }
            System.out.println();
        }
    }
}

