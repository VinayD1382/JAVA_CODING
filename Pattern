Pattern 11 : 
* 
* * 
* * * 
* * * * 
* * * * * 
* * * * * 
* * * * 
* * * 
* * 
* 
class Main{
    public static void main(String[] args){
        int count = 1;
        int n = 5;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print("* ");
            }
            System.out.println();
        }
        for(int i=n;i>=1;i--){
            for(int j=1;j<=i;j++){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}

Pattern 12 : 
- - - - * 
- - - * * 
- - * * * 
- * * * * 
* * * * * 
class Main{
    public static void main(String[] args){
        int count = 1;
        int n = 5;
        for(int i=1;i<=n;i++){
            for(int j=1;j<=n-i;j++){
                System.out.print("- ");
            }
            for(int k=1;k<=i;k++){
                System.out.print("* " );
            }
            System.out.println();
        }
    }
}
