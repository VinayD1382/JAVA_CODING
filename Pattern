10101
10101
10101
class Main {
    public static void main(String[] args) {
        int count = 1;
      for(int i=1;i<=5;i++){
          for(int j=1;j<=5;j++){
              if(j%2==0){
              System.out.print(0);
          }else{
              System.out.print(1);
          }
         
      }
       System.out.println();
    }
}
}
Pattern 5 : 
99999
96969
99999
96969
99999
class Main {
    public static void main(String[] args) {
        int count = 1;
      for(int i=1;i<=5;i++){
          for(int j=1;j<=5;j++){
              if(i%2==0 && j%2==0){
              System.out.print(6);
          }else{
              System.out.print(9);
          }
         
      }
       System.out.println();
    }
}
}

